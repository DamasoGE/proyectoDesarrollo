    <!-- Definir padding como Insets -->
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    
    <Label fx:id="labelSaludo" text="Hola JavaFX!" />
    <Button onAction="#handleBotonClick" text="Haz clic" />
   <Button onAction="#handleBotonClick2" text="Haz Click 2" />
</VBox>
